{
  "name": "art-configurator",
  "license": "Commercial License",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p dev:css vite",
    "build:win": "yarn vite build --mode development",
    "build:dev": "yarn run build:css && tsc && NODE_ENV=development vite build --mode development",
    "build:prod": "yarn run build:css && tsc && NODE_ENV=production vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "vite": "vite",
    "build:css": "yarn run generate:css --minify",
    "dev:css": "tailwindcss -w -i ./src/input.css -o ./src/output.css",
    "generate:css": "tailwindcss -i ./src/input.css -o ./src/output.css"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^41.2.1",
    "@ckeditor/ckeditor5-react": "^6.2.0",
    "@types/react-router-dom": "^5.3.3",
    "antd": "^5.16.1",
    "axios": "^1.6.8",
    "dompurify": "^3.0.11",
    "flowbite": "^2.3.0",
    "lorem-ipsum": "^2.0.8",
    "mobx": "^6.12.1",
    "mobx-react": "^9.1.0",
    "mobx-react-lite": "^4.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.2"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.20",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "npm-run-all": "^4.1.5",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.1"
  }
}
