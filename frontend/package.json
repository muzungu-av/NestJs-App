{
  "name": "pictures",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:css\" \"rsbuild dev --open\"",
    "build": "npm run build:css && rsbuild build",
    "build:css": "npm run generate:css -- --minify",
    "preview": "rsbuild preview",
    "dev:css": "tailwindcss -w -i ./src/input.css -o ./src/output.css",
    "generate:css": "tailwindcss -i ./src/input.css -o ./src/output.css"
  },
  "dependencies": {
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.6",
    "flowbite": "^2.2.1",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router": "^6.21.3",
    "react-router-dom": "^6.21.3"
  },
  "devDependencies": {
    "@rsbuild/core": "^0.3.6",
    "@rsbuild/plugin-react": "^0.3.6",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0"
  }
}
